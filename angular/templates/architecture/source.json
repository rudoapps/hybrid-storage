  "metadata": {
    "description": "Mapping between hybrid-storage repository templates and local project structure",
    "architecture": "Clean Architecture with Angular",
    "templateEngine": "Mustache",
    "layers": ["domain", "data", "infrastructure"],
    "lastUpdated": "2025-01-27"
  }
